{
  "name": "cli-utils",
  "version": "0.1.0",
  "private": true,
  "description": "Shared CLI progress utilities",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "^5.4.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "bunup": "latest"
  }
}
